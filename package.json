  },
  "homepage": "https://github.com/racker/Minerva#readme",
  "scripts": {
    "ng": "ng",
    "start:dev": "node ./helpers/setup.js",
    "prestart:mock": "npm run mocksetup",
    "start:mock": "ng serve -c mock --baseHref=/intelligence --public-host dev.i.rax.io -o",
    "build": "make build-frontend",
    "prebuild:mock": "npm run mocksetup",
    "build:mock": "ng build --watch=false -c=mock",
    "test:unit": "npm run mocksetup && ng test --project=minerva -c=mock --watch=false",
    "test:unit-ci": "ng test --project=minerva -c=mock --watch=false",
    "test:admin": "ng test --project=admin -c=mock",
    "test:admin-ci": "ng test --project=admin -c=mock --watch=false",
    "test:admin-e2e": "ng e2e --project=admin -c=mock ",
    "lint": "ng lint",
    "mocksetup": "node ./helpers/createMocks.js",
    "test:e2e": "npm run mocksetup && ng e2e --project=minerva -c=mock",
    "report": "allure generate allure-results --clean -o allure-report || true",
    "test:e2e-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 85.0.4183.87 && ng e2e --project=minerva -c=mock",
    "docs": "npx compodoc -p tsconfig.json -d docs"
  },
  "description": "Rackspace NextGen Intelligence",
  "main": "app.js",
  "engines": {
    "node": "10.0.0",
    "npm": "5.6.0"
  },
  "private": true,
